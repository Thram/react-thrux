{
  "name": "react-thrux",
  "version": "0.0.0-development",
  "description": "Thrux implementation for React components",
  "main": "dist/index.js",
  "scripts": {
    "tape": "tape-watch -r babel-register __test__/react-thrux.test.js",
    "test": "npm run tape -- -p | tap-spec --color",
    "test:single": "npm run tape -- --once -p | tap-spec --color",
    "commit": "git add . && git-cz",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "precommit": "npm run test:single",
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --out-dir dist src",
    "build:umd": "webpack --output-filename react-thrux.umd.js",
    "build:umd.min": "webpack --output-filename react-thrux.umd.min.js -p"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Thram/react-thrux.git"
  },
  "keywords": [
    "react-thrux",
    "react",
    "thrux"
  ],
  "author": "Thram <thramposo@gmail.com> (https://thram.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Thram/react-thrux/issues"
  },
  "homepage": "https://github.com/Thram/react-thrux#readme",
  "dependencies": {
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "thrux": "^2.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "^2.7.1",
    "npm-run-all": "^4.0.2",
    "react-addons-test-utils": "^15.4.2",
    "rimraf": "^2.6.1",
    "semantic-release": "^6.3.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tape-watch": "^2.2.4",
    "umd": "^3.0.1",
    "webpack": "^2.2.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
